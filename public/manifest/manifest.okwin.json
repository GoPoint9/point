{
	"manifest_version": 2,
	"name": "OK.Win",
	"version": "1.2",
	"display": "standalone",
	"background_color": "#fff",
	"description": "game for ok.win",
	"icons": [
		{
			"src": "/manifest/okwin@192.png?v1",
			"sizes": "192x192"
		},
		{
			"src": "/manifest/okwin@512.png?v1",
			"sizes": "512x512"
		}
	],
	"start_url": "/",
	"safari_specific": {
		"safari_extension_version": 2,
		"permissions": ["cross-origin-content", "web-page"]
	}
}
